{
  "name": "blink-frontend",
  "version": "0.1.0",
  "private": false,
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext 'js,jsx,ts,tsx' --cache --no-error-on-unmatched-pattern --fix",
    "style": "prettier --log-level warn --ignore-unknown --no-error-on-unmatched-pattern --cache",
    "style:check": "pnpm style --check",
    "style:format": "pnpm style --write",
    "types:check": "tsc --noEmit",
    "deps:prepare": "npm --prefix ../blink-backend run deps:up && npm --prefix ../blink-backend run migration:apply",
    "deps:up": "npm --prefix ../blink-backend run deps:up && npm --prefix ../blink-backend run dev"
  },
  "dependencies": {
    "next": "14.2.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.13",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.6.2"
  }
}
